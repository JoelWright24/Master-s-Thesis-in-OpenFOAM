transportModel  Newtonian;

nu              nu [0 2 -1 0 0 0 0] 1.896e-05; //air at 60 degrees Celcius

Cv              Cv [0 2 -2 -1 0 0 0] 718;

rho0            rho0 [1 -3 0 0 0 0 0] 1.225;

kappa           kappa [1 1 -3 -1 0 0 0] 0.0242;

beta            beta [0 0 0 -1 0 0 0] 0.00021;

theta0          theta0 [0 0 0 1 0 0 0] 293;

g               g [0 1 -2 0 0 0 0] (0.0 -9.81 0.0);


