FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    
    wallPatch
    {
        type            noSlip;
    }
/*    fluid_to_solid
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
*/
    filter
    {
        type            noSlip;
    }
    inletPatch
    {
        type            fixedValue;
        value           uniform (0 0 -12.5);
    }
    outletPatch
    {
        type            zeroGradient;
        //value           uniform (0 0 0);
        //inletValue      uniform (0 0 0);
    }
}

